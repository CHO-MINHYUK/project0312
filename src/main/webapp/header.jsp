<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<style>
	body,html{
		   height:100%;
		   width:100%;
	}
	header{
		   background-color:lightgray;
		   height:15%;
		   text-align:center;
		   font-size:1.5em;
		   line-height:140px
	}
	footer{
		   height:5%;
		   background-color:gray;
		   text-align:center;
		   line-height:45px
	}
	nav{
	    background-color:black;
	    height:5%;
	    line-height:48px
	}
	section{
		    height:75%;
	}
	*{
	  padding:0;
	  margin:0;	
	}
	nav ul{
		   list-style-type:none;
		   display:inline-block;
	}
	nav li{
		   float:left;
	}
	nav a{
		  padding:0 20px;
		  color:white;
		  text-decoration:none;
	}
	table {
    margin-left:auto; 
    margin-right:auto;
	}
	table, tr, td {
    	border-collapse : collapse;
        border : 1px solid black;
	}
	h2{
	   text-align:center;
	}
</style>
</head>
<body>
<header>
	<h2>회원 매출 관리 프로그램</h2>
</header>
</body>
</html>